public class MaxInArray {
    public static void main(String[] args) {
        int[] numbers = {10, 25, 8, 99, 76};
        int max = numbers[0];
        
        for (int num : numbers) {
            if (num > max) {
                max = num;
            }
        }
        
        System.out.println("Maximum number: " + max);
    }
}
